# Staging Environment Configuration

# API Configuration
VITE_API_URL=/api
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Application Configuration
VITE_APP_NAME="Claude-to-Azure Proxy (Staging)"
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION="AI Chat Interface - Staging"
VITE_APP_ENVIRONMENT=staging

# Feature Flags (Staging)
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_OFFLINE_SUPPORT=true

# Security Configuration
VITE_CSP_REPORT_URI=""
VITE_SENTRY_DSN=""
VITE_SECURITY_HEADERS=true

# CDN Configuration
VITE_CDN_URL=""
VITE_STATIC_URL=""
VITE_ASSETS_BASE_URL=""

# Performance Optimizations
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_PRELOAD=true
VITE_ENABLE_PREFETCH=true
VITE_CHUNK_SIZE_WARNING_LIMIT=1000

# Staging Optimizations
VITE_DEV_TOOLS=true
VITE_MOCK_API=false
VITE_BUILD_ANALYZE=false
VITE_SOURCE_MAPS=true
VITE_MINIFY_CSS=true
VITE_MINIFY_JS=true
VITE_TREE_SHAKING=true

# Cache Configuration
VITE_CACHE_STATIC_ASSETS=true
VITE_CACHE_API_RESPONSES=false
VITE_SW_CACHE_STRATEGY=networkFirst

# Monitoring and Observability
VITE_ENABLE_WEB_VITALS=true
VITE_PERFORMANCE_BUDGET_JS=500
VITE_PERFORMANCE_BUDGET_CSS=100
VITE_PERFORMANCE_BUDGET_IMAGES=1000

# Build Configuration
VITE_BUILD_TARGET=es2022
VITE_BUILD_POLYFILLS=false
VITE_BUILD_LEGACY=false